{
  "name": "testnodejs",
  "version": "1.0.0",
  "private": true,
  "description": "Full stack node js test project for UKESS ",
  "main": "app.js",
  "scripts": {
    "prestart:dev": "webpack --mode development --config config/webpack.base.config.js  --env.NODE_ENV=developer --progress",
    "prestart:prod": "webpack --mode production --config config/webpack.prod.config.js --env.NODE_ENV=production --progress",
    "start:dev": "concurrently \"npm run start:dev-server\" \"node app.js\"",
    "start:prod": "node app.js",
    "start:dev-server": "webpack-dev-server --mode development --config config/webpack.base.config.js --open --hot --history-api-fallback --env.NODE_ENV=developer"
  },
  "author": "Yurii Pleshynets",
  "license": "MIT",
  "dependencies": {
    "acorn": "^6.1.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "mysql2": "^1.6.5",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "rest-api-errors": "^1.2.1"
  },
  "devDependencies": {
    "axios": "^0.9.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "compression-webpack-plugin": "^2.0.0",
    "concurrently": "^4.1.0",
    "css-loader": "^0.28.11",
    "es6-promise": "^4.0.5",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.6.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2",
    "webpack-stream": "^5.2.1"
  }
}
